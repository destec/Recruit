<div class="pageContent">
  <form method="post" action="demo/common/ajaxDone.html" class="pageForm required-validate" onsubmit="return validateCallback(this, navTabAjaxDone);">
    <div class="pageFormContent" layoutH="56">
      <p>
        <label>系统编号：</label>
        <input name="sn" type="text" size="30" class="digits" readonly value="36" value="37"/>
      </p>
      <p>
        <label>编号：</label>
        <input name="sn" type="text" size="30" class="digits required" value="<%= student.code %>" />
      </p>
      <p>
        <label>姓名：</label>
        <input name="name" type="text" size="30" class=" required" value="<%= student.name %>" />
      </p>
      <p>
        <label>性别：</label>
        <input name="sex" type="radio" size="30" checked />男
        <input name="sex" type="radio" size="30" />女
      </p>
      <p>
        <label>家庭住址：</label>
        <input name="name" type="text" size="30" value="<%= student.address %>" class=" required"/>
      </p>
      <p>
        <label>联系电话：</label>
        <input name="name" type="text" size="30" value="<%= student.phone %>" class=" required"/>
      </p>
      <p>
        <label>出生时间：</label>
        <input name="name" type="text" size="30" class="date" value="<%= student.birthday %>"/>
      </p>
      <p>
        <label>民族：</label>
        <input name="name" type="text" size="30" value="<%= student.nation %>"/>
      </p>
      <p>
        <label>录取院系：</label>
        <input name="name" type="text" size="30" value="<%= student.department %>" class=" required"/>
      </p>
      <p>
        <label>录取专业：</label>
        <input name="name" type="text" size="30" value="<%= student.speciality %>" class=" required"/>
      </p>
      <p>
        <label>年制：</label>
        <input name="name" type="text" size="30" value="<%= student.year %>"/>
      </p>
      <p>
        <label>是否住宿：</label>
        <input name="accommodation" type="radio" size="30" checked />是
        <input name="accommodation" type="radio" size="30" />否
      </p>
      <p>
        <label>毕业学校：</label>
        <input name="name" type="text" size="30" value="<%= student.graduateschool %>" class=" required"/>
      </p>
      <p>
        <label>退学时间：</label>
        <input name="name" type="text" size="30" class="date required" value=""/>
      </p>
      <p>
        <label>备注：</label>
        <textarea name="textarea1" cols="102" rows="4" class="textInput"></textarea>
      </p>
    </div>
    <div class="formBar">
      <ul>
        <!--<li><a class="buttonActive" href="javascript:;"><span>保存</span></a></li>-->
        <li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
        <li>
          <div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div>
        </li>
      </ul>
    </div>
  </form>
</div>
