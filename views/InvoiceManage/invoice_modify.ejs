<div class="pageContent">
  <form method="post" action="demo/common/ajaxDone.html" class="pageForm required-validate" onsubmit="return validateCallback(this, navTabAjaxDone);">
    <div class="pageFormContent" layoutH="56">
      <p>
        <label>编号：</label>
        <input name="sn" type="number" size="30" class="required digits" alt="请输入编号" value="<%= invoice.code %>"/>
      </p>
      <p>
        <label>姓名：</label>
        <input name="sn" type="text" size="30" class="required digits" alt="请输入学生姓名" value="<%= invoice.name %>"/>
      </p>
      <p>
        <label>系别：</label>
        <select>
          <option>机电系</option>
        </select>
      </p>
      <p>
        <label>专业：</label>
        <select>
          <option>计算机网络(网络软件工程师)</option>
        </select>
      </p>
      <p>
        <label>性别：</label>
        <select>
          <option>男</option>
          <option>女</option>
        </select>
      </p>
      <p>
        <label>发票号码：</label>
        <input name="sn" type="text" size="30" class="required digits" alt="请输入发票号码" value="<%= invoice.invoicenumber %>"/>
      </p>
      <p>
        <label>预售款：</label>
        <input name="sn" type="text" size="30" class="required digits" alt="请输入预售款" value="<%= invoice.invoicebill %>"/>
      </p>
      <p>
        <label>凭证号：</label>
        <input name="sn" type="text" size="30" class="required digits" alt="请输入凭证号" value="<%= invoice.invoicecode %>" />
      </p>
      <p>
        <label>开票日期：</label>
        <input name="sn" type="text" size="30" class="required date" alt="请输入开票日期" value="<%= invoice.invoicedate %>"/>
      </p>
    </div>
    <div class="formBar">
      <ul>
        <!--<li><a class="buttonActive" href="javascript:;"><span>保存</span></a></li>-->
        <li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
        <li>
          <div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div>
        </li>
      </ul>
    </div>
  </form>
</div>
