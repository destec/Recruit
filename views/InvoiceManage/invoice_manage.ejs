<form id="pagerForm" method="post" action="demo_page1.html">
  <input type="hidden" name="status" value="${param.status}">
  <input type="hidden" name="keywords" value="${param.keywords}" />
  <input type="hidden" name="pageNum" value="1" />
  <input type="hidden" name="numPerPage" value="${model.numPerPage}" />
  <input type="hidden" name="orderField" value="${param.orderField}" />
</form>


<div class="pageHeader">
  <form onsubmit="return navTabSearch(this);" action="demo_page1.html" method="post">
  <div class="searchBar">
    <table class="searchContent">
      <tr>
        <td>
          姓名：<input type="text" name="keyword" />
          凭证号：<input type="text" name="keyword" />
      </tr>
    </table>
    <div class="subBar">
      <ul>
        <li><div class="buttonActive"><div class="buttonContent"><button type="submit">检索</button></div></div></li>
      </ul>
    </div>
  </div>
  </form>
</div>
<div class="pageContent">
  <div class="panelBar">
    <ul class="toolBar">
      <li><a class="add" href="invoice_add" target="dialog" mask="true" height="400"><span>添加</span></a></li>
      <li><a class="delete" href="invoice_del?uid={sid_invoice}" target="ajaxTodo" title="确定要删除吗?"><span>删除</span></a></li>
      <li><a class="edit" href="invoice_modify?uid={sid_invoice}" target="dialog" mask="true" height="400"><span>修改</span></a></li>
      <li class="line">line</li>
    </ul>
  </div>
  <table class="table" width="100%" layoutH="115">
    <thead>
      <tr>
        <th></th>
        <th width="8%">编号</th>
        <th width="10%">姓名</th>
        <th width="10%">系别</th>
        <th width="20%">专业</th>
        <th width="5%">性别</th>
        <th width="15%">发票号码</th>
        <th width="10%">预售款</th>
        <th width="10%">凭证号</th>
        <th width="10%">开票日期</th>
      </tr>
    </thead>
    <tbody>
      <% for(var i=0; i<invoices.length; i++) { %>
        <tr target="sid_invoice" rel="<%= invoices[i].id %>">
          <td><%= invoices[i].id %></td>
          <td><%= invoices[i].code %></td>
          <td><%= invoices[i].name %></td>
          <td><%= invoices[i].department %></td>
          <td><%= invoices[i].speciality %></td>
          <td><%= invoices[i].sex %></td>
          <td><%= invoices[i].invoicenumber %></td>
          <td><%= invoices[i].invoicebill %></td>
          <td><%= invoices[i].invoicecode %></td>
          <td><%= invoices[i].invoicedate %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
  </div>
</div>
