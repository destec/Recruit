<div class="pageContent">
  <form method="post" action="demo/common/ajaxDone.html" class="pageForm required-validate" onsubmit="return validateCallback(this, navTabAjaxDone);">
    <div class="pageFormContent" layoutH="56">
      <p>
        <label>系统编号：</label>
        <input name="sn" type="text" size="30" class="digits" readonly value="<%= student.id %>"/>
      </p>
      <p>
        <label>编号：</label>
        <input name="sn" type="text" size="30" class="digits" readonly value="<%= student.code1 %>"/>
      </p>
      <p>
        <label>姓名：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.name %>"/>
      </p>
      <p>
        <label>性别：</label>
        <input name="sex" type="radio" size="30" readonly checked />男
        <input name="sex" type="radio" size="30" readonly />女
      </p>
      <p>
        <label>家庭住址：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.address %>"/>
      </p>
      <p>
        <label>联系电话：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.phone %>"/>
      </p>
      <p>
        <label>出生时间：</label>
        <input name="name" readonly type="text" size="30" class="date" value="<%= student.birthday %>"/>
      </p>
      <p>
        <label>民族：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.nation %>"/>
      </p>
      <p>
        <label>毕业学校：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.graduateschool %>"/>
      </p>
      <p>
        <label>毕业班主任：</label>
        <input name="name" readonly type="text" size="30" class="" value="<%= student.graduateperson %>"/>
      </p>
      <p>
        <label>中考准考证号：</label>
        <input name="name" readonly type="text" size="30" class="" value="<%= student.code2 %>"/>
      </p>
      <p>
        <label>课目1：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.score1 %>"/>
      </p>
      <p>
        <label>课目2：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.score2 %>"/>
      </p>
      <p>
        <label>课目3：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.score3 %>"/>
      </p>
      <p>
        <label>课目4：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.score4 %>"/>
      </p>
      <p>
        <label>课目5：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.score5 %>"/>
      </p>
      <p>
        <label>课目6：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.score6 %>"/>
      </p>
      <p>
        <label>课目7：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.score7 %>"/>
      </p>
      <p>
        <label>加分：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.extrascore %>"/>
      </p>
      <p>
        <label>总分1：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.totalscore1 %>"/>
      </p>
      <p>
        <label>总分2：</label>
        <input name="name" readonly type="text" size="30" value="<%= student.totalscore2 %>"/>
      </p>
      <p>
        <label>第一志愿：</label>
        <input name="name" readonly type="text" size="30" class="" value="<%= student.first %>"/>
      </p>
      <p>
        <label>第二志愿：</label>
        <input name="name" readonly type="text" size="30" class="" value="<%= student.second %>"/>
      </p>
    </div>
    <div class="formBar">
      <ul>
        <!--<li><a class="buttonActive" href="javascript:;"><span>保存</span></a></li>-->
        <li>
          <div class="button"><div class="buttonContent"><button type="button" class="close">确定</button></div></div>
        </li>
      </ul>
    </div>
  </form>
</div>
