<div class="pageContent">
	<form method="post" action="demo/common/ajaxDone.html" class="pageForm required-validate" onsubmit="return validateCallback(this, navTabAjaxDone);">
		<div class="pageFormContent" layoutH="56">
			<p>
				<label>工 号：</label>
				<input name="sn" type="text" size="30" class="required digits" value="<%= user.id %>"/>
			</p>
			<p>
				<label>姓 名：</label>
				<input name="name" class="required" type="text" size="30" class="required" value="<%= user.name %>"/>
			</p>
			<p>
				<label>性 别：</label>
				<select class="required combox">
            <option value="male">男</option>
            <option value="female">女</option>
        </select>
			</p>
			<p>
				<label>密 码：</label>
				<input id="pwd" type="password" class="required alphanumeric" minlength="6" maxlength="20" value="<%= user.password %>">
			</p>
            <p>
				<label>确认密码：</label>
				<input type="password" class="required alphanumeric" minlength="6" maxlength="20" equalto="#pwd" value="<%= user.password %>">
			</p>
			<p>
				<label>权 限：</label>
				<select class="required combox">
					<option value="admin">管理员</option>
					<option value="typer">输入员</option>
				</select>
			</p>
		</div>
		<div class="formBar">
			<ul>
				<!--<li><a class="buttonActive" href="javascript:;"><span>保存</span></a></li>-->
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li>
					<div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div>
				</li>
			</ul>
		</div>
	</form>
</div>
